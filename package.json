{
  "name": "site_exosummits",
  "description": "Exo Summit static website",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rimraf public",
    "deploy": "gatsby build; cd public; now --name website-exoleaders",
    "dev": "gatsby develop",
    "now-build": "npm run build",
    "format": "prettier --semi --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "deploy:gh-pages": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "babel-plugin-styled-components": "^1.10.6",
    "classnames": "^2.2.6",
    "country-region-selector": "^0.5.1",
    "country-state-city": "^1.0.0",
    "form-serialize": "^0.7.2",
    "gatsby": "^2.13.1",
    "gatsby-cli": "^2.7.7",
    "gatsby-image": "^2.0.5",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-google-analytics": "^2.0.10",
    "gatsby-plugin-purgecss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.2.2",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.2.1",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.2.1",
    "jsonp": "^0.2.1",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.0",
    "polished": "^2.0.3",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/form-serialize": "^0.7.0",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "gh-pages": "^2.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
  }
}
